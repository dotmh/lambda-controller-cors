{
  "name": "@dotmh/lambda-controller-cors",
  "version": "1.1.1",
  "description": "A Plugin to add Cors support to Lambda Controller",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "xo",
    "lint:md": "remark .",
    "docs": "jsdoc index.js -c ./jsdoc.json",
    "readdocs": "npm run docs && npx http-server ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotmh/lambda-controller-cors.git"
  },
  "keywords": [
    "Lambda",
    "Serverless",
    "DotMH",
    "Cors"
  ],
  "author": "Martin Haynes <oss@dotmh.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dotmh/lambda-controller-cors/issues"
  },
  "homepage": "https://github.com/dotmh/lambda-controller-cors#readme",
  "devDependencies": {
    "@dotmh/lambda-controller": "^1.2.4",
    "ink-docstrap": "^1.3.2",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "xo": "^0.32.0"
  },
  "peerDependencies": {
    "@dotmh/lambda-controller": "^1.2.4"
  },
  "xo": {
    "ignores": [
      "specs",
      "docs"
    ],
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "arrow-parens": [
        "error",
        "always"
      ]
    },
    "env": [
      "jest"
    ]
  },
  "remarkConfig": {
    "settings" : {
      "setext": true
    },
    "plugins": ["remark-preset-lint-recommended"]
  }
}
